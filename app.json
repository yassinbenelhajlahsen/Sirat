{
  "expo": {
    "name": "Sirat",
    "slug": "Sirat",
    "extra": {
      "fullName": "Sirat - The Path to Your Deen"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sirat_icon.png",
    "scheme": "sirat",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#0c3605",
    "newArchEnabled": true,
    "ios": {
      "userInterfaceStyle": "dark",
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Sirat"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen"
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.anonymous.Sirat"
    }
  }
}
